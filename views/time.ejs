<% if (test[0] === "12:00AM") { %>
    <div class="row">
        <label class="col-md-5 mt-3">DOCTOR NOT AVAILABLE</label>
    </div>
<%} else { %>
    <div class="row">
        <label class="col-md-5 mt-3">SELECT A TIME FOR APPOINTMENT<span class="r">*</span></label>
    </div>
   
    <% var i = test.length %>
    <% while(i--) { %>
        <% for (var j = 0; j < valdem.length;  j++ ) { %>
            <% if (test[i] === valdem[j].app_time) { %>
                <% test.splice(i,1) %>
                <% break %>
            <%}%>
        <%}%>   
    <%}%>

    <% if (test.length===0) { %>
        <div class="row">
            <label class="col-md-5 mt-3">ALL APPOINTMENTS ARE BOOKED</label>
        </div>
    <%} else { %>
        <div class="row btn-group btn-group-toggle" data-toggle="buttons" style="padding-left: 10%; padding-bottom: 1%; min-width: 100%;">
            <% for (var i =0; i < test.length;  i++ ) { %>
                <label class="col-md-2 btn btn-outline-primary mr-3 px-4 py-2 mt-3 rounded">
                    <input type="radio" name="app_time" value= "<%= test[i] %>" id="rad<%= i %>"><%= test[i] %>
                </label>
            <%}%>
        </div>
        <div class="form-group row mt-3">
            <label class="col-md-4">AILMENT/ SYMPTOMS<span class="r">*</span></label>
            <div class="col-md-12">
                <textarea name="allergies" id="allergies" cols="146" rows="10" placeholder="Enter your illness..." required></textarea>
            </div>
        </div>
        <input class="btn btn-success mt-2 px-5 btn-block" style="padding: 10px;" type="submit" name="submit" id="submit" value="SUBMIT">
    <%}%>
<%}%>